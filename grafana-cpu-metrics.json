        "justifyMode": "auto"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "avg(avg_over_time(cpu_usage{job=~\"$job\", instance=~\"$instance\"}[$interval]))",
          "legendFormat": "avg over $interval"
